    "cron": "^1.8.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "~4.16.1",
    "firebase-admin": "^9.1.1",
    "firebase-functions": "^3.11.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "minimist": "^1.2.0",
    "mongoose": "^5.10.4",
    "morgan": "~1.9.1",
